tUserProfile(currentUserProfile);\n        setUserHasProfile(true);\n        return foundUserProfile;\n      } else {\n        setUserHasProfile(false);\n      }\n    });\n  }\n\n  console.log(process.env.REACT_APP_MONGODB_USER_PROFILES_DATABASE_GET_ROUTE); \n\n  if (isAuthenticated){\n     getUserInformation(user)\n  }\n\n\n  //Function to handle the creation of a new User Profile\n  const handleNewUserProfileCreation = (newUserProfile) => { \n    const currentUserProfile = userProfile.concat(newUserProfile)\n    setUserHasProfile(true);\n    console.log(currentUserProfile)\n  }\n\n  let currentlyVisibleState = null;\n  let currentlyVisibleForm = null;\n\n  if(isAuthenticated && !userHasProfile){\n    currentlyVisibleForm =  <StarterQuestionnaire\n                              user = {user}\n                              onNewUserProfileCreation = {handleNewUserProfileCreation}/>\n    currentlyVisibleState = null\n  } else if(isAuthenticated && userHasProfile) {\n    currentlyVisibleForm = <DateInputForm/>\n    currentlyVisibleState = <TripInfo/> \n  } else {\n    currentlyVisibleForm = null\n    currentlyVisibleState = null\n  }\n  \n  return(\n    <React.Fragment>\n      {currentlyVisibleForm}\n      {currentlyVisibleState}\n    </React.Fragment>\n  )\n  \n};\n\nexport default TripInfoControl;","/Users/paulkasidit/Desktop/Epicodus/go/client/src/components/HeaderControl.js",["97","98","99","100","101","102"],[],"import React, { useState } from 'react';\nimport \"./Header/Header.css\"\nimport Navbar from './Header/NavBar'; \nimport SearchBar from './Header/SearchBar';\nimport AccountDetail from './Header/AccountDetail';\nimport AccountInfo from './Header/AccountInfo'; \nimport LoginButton from './Header/LoginButton';\nimport LogoutButton from './Header/LogoutButton';\nimport { faListCheck, faTruckField, faTruckFieldUn } from '@fortawesome/free-solid-svg-icons';\nimport { useAuth0 } from '@auth0/auth0-react';\n\nfunction HeaderControl(){\n\n  const {user, isAuthenti